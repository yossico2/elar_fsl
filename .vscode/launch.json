{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug fsl",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/linux/debug/fsl",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/linux/debug",
            "environment": [
                {
                    "name": "LOGGING_LEVEL",
                    "value": "DEBUG"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}