{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug fsl (build-debug)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-debug/fsl",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build-debug",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}